<?php
class MContentHeader extends MControl
{
    var $object;
    var $target;
    function __construct($object, $target)
    {
        parent::__construct();
        $this->object = $object;
        $this->target = $target;
    }

    function Generate()
    {
        $div = new MDiv('', $this->object . ': ' . $this->target, 'm-content-header');
        return $div->Generate();
    }
}
?>