//novo
	function btnReport_click() 
	{  
	$ui = $this->manager->GetUI(); 
	if ( $this->processo->andamento ) 
	{ 
		global $MIOLO; 
		$report = new MJasperReport('sigaept'); 
		$parameters['SUBREPORT_DIR'] = $MIOLO->GetConf('home.modules'); 
		$parameters['pNumProcesso']  = $this->processo->numprocesso(); 
		$parameters['pURL']          = $MIOLO->GetConf('home.url'); 
		$parameters['pTitulo']       = 'TRAMITE DE PROCESSO'; 
		$parameters['pUser']         = $MIOLO->login->idkey; 
		$parameters['andamento']     = '%'; 
		$report->Execute('protocolo', 'ProcessoTramite', $parameters); 
	} 
	else 
	{ 
		$this->addError('Não há andamentos'); 
	}
	} 

