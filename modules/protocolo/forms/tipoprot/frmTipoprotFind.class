<?
MIOLO::Import('modules::protocolo::controls::linktipoprot');

class frmTipoprotFind extends Form
{
    function __construct()
    {
	global $MIOLO, $module;
        $this->Form('Pesquisar Tipos Protocolados');
	$this->SetClose($MIOLO->GetActionURL($module,'main:configuracao'));
        $this->EventHandler();
    }

    function CreateFields()
    {
        $ui = $this->manager->GetUI();
        $grid = $ui->GetGrid('protocolo','gridTipoprot');
        $grid->SetTitle(NULL);
        
        $findButton = new ButtonFind('');
        $findButton->SetName('btnFind');
        
	$newLink = new LinkTipoprot('lnkNew',FALSE);
        $newLink->setNew();

        $tipo = Form::GetFormValue('tipo');
        $formBusca = new MThemeBox('&nbsp;&nbsp;Busca Simples', array(new MText('','Tipo Protocolado'),new MTextField('tipo',$tipo,'',30),new ButtonFind('')));
        $formBusca->width = '60%';
        $formBusca->float = 'left';
        $formBusca->padding = '6px';
        $formBusca->height = '50px';
        $fields = array(
            array(
                $formBusca
            ),
            $newLink,
            $grid,
        );
        $this->SetFields($fields);
        $this->defaultButton = false;
    }
 }
?>
