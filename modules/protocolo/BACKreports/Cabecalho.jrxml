	<parameter name="pUser" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select 	i.instituicao, i.nome
from 	cm_instituicao i, cm_usuario u 
where 	i.idinstituicao = u.idinstituicao
and	u.idusuario = $P{pConsulta}]]></queryString>

	<field name="instituicao" class="java.lang.String"/>
