				<textFieldExpression   class="java.lang.String"><![CDATA["RELATÓRIO DE " + $P{pTitulo}+ "  -  " + new SimpleDateFormat( "dd/MM/yyyy" ).format( new Date( ) )]]></textFieldExpression>
				</textField>
