<map>
    <moduleName>protocolo</moduleName>
    <className>andamento</className>
    <tableName>ad_andamento</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>numProcesso</attributeName>
        <columnName>numpro</columnName>
		<key>primary</key>
    </attribute>
<!--  -->
	<attribute>
        <attributeName>idUsuario</attributeName>
        <columnName>idusuario</columnName>
    </attribute>
    <attribute>
        <attributeName>numAndamento</attributeName>
        <columnName>numanda</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>dataEntrada</attributeName>
        <columnName>stampentr</columnName>
    </attribute>
    <attribute>
        <attributeName>dataSaida</attributeName>
        <columnName>stampsai</columnName>
    </attribute>
    <attribute>
        <attributeName>dataDespacho</attributeName>
        <columnName>stampdesp</columnName>
    </attribute>
    <attribute>
        <attributeName>status</attributeName>
        <columnName>inativo</columnName>
    </attribute>
    <attribute>
        <attributeName>setor</attributeName>
        <columnName>setor</columnName>
    </attribute>
    <attribute>
        <attributeName>setorDestino</attributeName>
        <columnName>setordest</columnName>
    </attribute>
    <attribute>
        <attributeName>decisao</attributeName>
        <columnName>despacho</columnName>
        <converter>
		<converterName>CaseConverter</converterName>
		<parameter>
			<parameterName>case</parameterName>
			<parameterValue>upper</parameterValue>
		</parameter>
	    </converter>
    </attribute>
    <attribute>
        <attributeName>autor</attributeName>
        <columnName>autor</columnName>
        <converter>
		<converterName>CaseConverter</converterName>
		<parameter>
			<parameterName>case</parameterName>
			<parameterValue>upper</parameterValue>
		</parameter>
	    </converter>
    </attribute>
    <attribute>
        <attributeName>processo</attributeName>
    </attribute>
   <association>
		<toClassModule>protocolo</toClassModule>
		<toClassName>processo</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>processo</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>numProcesso</fromAttribute>
			<toAttribute>numProcesso</toAttribute>
		</entry>
   </association>
</map>
