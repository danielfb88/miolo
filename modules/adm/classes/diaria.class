class BusinessAdmDiaria extends Business
{
	public $categoriaDiaria;
	public $categoriaFunc;
	public $diaria;
    public $meiaDiaria;

	function __construct($data = null)
	{
		parent::__construct('sigaept', $data);
	}

    function OnCreate($data)
    {
        if (empty($data))
        {
//            $this->initialize();
        }
        else
        {
            $this->GetById($data);
        }
    }

	function GetById($id1,$id2)
	{
		$this->categoriaDiaria = trim($id1);
    	$this->categoriaFunc = trim($id2);    
		$this->retrieve();
		return $this;
	}

	function ListAll()
	{
        $criteria =  $this->getCriteria();
        return $criteria->retrieveAsQuery();
	}
}

	
