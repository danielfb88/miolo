<map>
	<moduleName>adm</moduleName>
	<className>ordementrega</className>
	<tableName>ad_ordentrega</tableName>
	<databaseName>sigaept</databaseName>
	<attribute>
		<attributeName>idOrdemEntrega</attributeName>
		<columnName>idordement</columnName>
		<idgenerator>seq_ad_ordentrega</idgenerator>
		<key>primary</key>
	</attribute>
	<attribute>
		<attributeName>data</attributeName>
		<columnName>dataordent</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>numero</attributeName>
		<columnName>numeroordem</columnName>
	</attribute>
	<attribute>
		<attributeName>idUoExecutante</attributeName>
		<columnName>iduoexec</columnName>
	</attribute>
	<attribute>
		<attributeName>impresso</attributeName>
		<columnName>impresso</columnName>
	</attribute>
	<attribute>
		<attributeName>movimento</attributeName>
	</attribute>
    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>movimento</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>movimento</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idOrdemEntrega</fromAttribute>
			<toAttribute>idOrdemEntrega</toAttribute>
		</entry>
    </association>
</map>
