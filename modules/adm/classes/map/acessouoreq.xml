<map>
    <moduleName>adm</moduleName>
    <className>acessouoreq</className>
    <tableName>ad_acessouoreq</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idUsuario</attributeName>
        <columnName>idusuario</columnName>
    </attribute>
    <attribute>
        <attributeName>idUo</attributeName>
        <columnName>iduo</columnName>
    </attribute>
    <attribute>
        <attributeName>uo</attributeName>
    </attribute>

   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>uo</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>uos</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<entry>
			<fromAttribute>idUo</fromAttribute>
			<toAttribute>idUo</toAttribute>
		</entry>
   </association>
   
   <association>
		<toClassModule>common</toClassModule>
		<toClassName>usuario</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>usuarios</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<entry>
			<fromAttribute>idUsuario</fromAttribute>
			<toAttribute>idUsuario</toAttribute>
		</entry>
   </association>
   
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>uo</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>uo</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idUo</fromAttribute>
			<toAttribute>idUo</toAttribute>
		</entry>
   </association>

</map>
