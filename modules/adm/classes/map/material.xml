<map>
	<moduleName>adm</moduleName>
	<className>material</className>
	<tableName>ad_material</tableName>
	<databaseName>sigaept</databaseName>
	<attribute>
		<attributeName>idMaterial</attributeName>
		<columnName>idmaterial</columnName>
		<idgenerator>seq_ad_material</idgenerator>
    	<key>primary</key>
	</attribute>
	<attribute>
		<attributeName>descricao</attributeName>
		<columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>unidade</attributeName>
		<columnName>unidade</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
        <attribute>
		<attributeName>precoestimado</attributeName>
		<columnName>precoestimado</columnName>
	</attribute>
	<attribute>
		<attributeName>permcons</attributeName>
		<columnName>permcons</columnName>
	</attribute>
	<attribute>
		<attributeName>estocavel</attributeName>
		<columnName>estocavel</columnName>
	</attribute>
	<attribute>
		<attributeName>idelemento</attributeName>
