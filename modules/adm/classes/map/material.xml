	<attribute>
		<attributeName>descresum</attributeName>
		<columnName>descresum</columnName>
	</attribute>
	<attribute>
		<attributeName>ativo</attributeName>
		<columnName>ativo</columnName>
	</attribute>
    
	<attribute>
		<attributeName>subelemento</attributeName>
	</attribute>
	<attribute>
		<attributeName>elemento</attributeName>
	</attribute>

    <association>
      <toClassModule>adm</toClassModule>
      <toClassName>itemreq</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>material</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idMaterial</fromAttribute>
	     <toAttribute>idMaterial</toAttribute>
      </entry>
   </association>
   
    <association>
      <toClassModule>adm</toClassModule>
      <toClassName>subelemento</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>subelemento</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idelemento</fromAttribute>
	     <toAttribute>idElemento</toAttribute>
      </entry>
	  <entry>
	     <fromAttribute>idsubelemento</fromAttribute>
	     <toAttribute>idSubElemento</toAttribute>
      </entry>
	</association>

	<association>
      <toClassModule>adm</toClassModule>
      <toClassName>elemento</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>elemento</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idelemento</fromAttribute>
	     <toAttribute>idElemento</toAttribute>
      </entry>
   </association>

</map>
