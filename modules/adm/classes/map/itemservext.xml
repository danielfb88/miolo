<map>
    <moduleName>adm</moduleName>
    <className>itemservext</className>
    <tableName>ad_itemreqservext</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idRequisicao</attributeName>
        <columnName>idrequisicao</columnName>
    	<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idServico</attributeName>
        <columnName>idservico</columnName>
    </attribute>
    <attribute>
        <attributeName>complemento</attributeName>
        <columnName>complemento</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>quantidade</attributeName>
        <columnName>quantidade</columnName>
    </attribute>
    <attribute>
        <attributeName>quantEmpenhada</attributeName>
        <columnName>quantempenhada</columnName>
    </attribute>
    <attribute>
        <attributeName>quantEntregue</attributeName>
        <columnName>quantentregue</columnName>
    </attribute>
    <attribute>
        <attributeName>quantDevolvida</attributeName>
        <columnName>quantdevolvida</columnName>
    </attribute>
    <attribute>
        <attributeName>valorEst</attributeName>
        <columnName>valorest</columnName>
    </attribute>   
    <attribute>
        <attributeName>valorReal</attributeName>
        <columnName>valorreal</columnName>
    </attribute>
    <attribute>
        <attributeName>idLicitacao</attributeName>
        <columnName>idlicitacao</columnName>
    </attribute>
    <attribute>
        <attributeName>item</attributeName>
        <columnName>item</columnName>
    </attribute>
     <attribute>
        <attributeName>prioridade</attributeName>
        <columnName>prioridade</columnName>
    </attribute>
     <attribute>
        <attributeName>status</attributeName>
        <columnName>status</columnName>
    </attribute>
	<attribute>
		<attributeName>unidade</attributeName>
		<columnName>unidade</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>

	<attribute>
        <attributeName>servico</attributeName>
    </attribute>
    <attribute>
        <attributeName>requisicao</attributeName>
    </attribute>
    <attribute>
        <attributeName>mapa</attributeName>
    </attribute>
    <attribute>
        <attributeName>empenhado</attributeName>
    </attribute>
    
    <association>
      <toClassModule>adm</toClassModule>
      <toClassName>requisicao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>requisicao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idRequisicao</fromAttribute>
	     <toAttribute>idRequisicao</toAttribute>
      </entry>
   </association>

	<association>
		<toClassModule>adm</toClassModule>
		<toClassName>servico</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>servico</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idServico</fromAttribute>
			<toAttribute>idServico</toAttribute>
		</entry>
	</association>
    <association>
      <toClassModule>adm</toClassModule>
      <toClassName>requisicao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>requisicao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idRequisicao</fromAttribute>
	     <toAttribute>idRequisicao</toAttribute>
      </entry>
   </association>
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>mapalicitacao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>mapa</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
		<entry>
			<fromAttribute>item</fromAttribute>
			<toAttribute>item</toAttribute>
		</entry>
    </association>
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>empenhado</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>empenhado</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
		<entry>
			<fromAttribute>item</fromAttribute>
			<toAttribute>item</toAttribute>
		</entry>
    </association>
		<association>
		<toClassModule>adm</toClassModule>
		<toClassName>licitacao</toClassName>
		<cardinality>ManyToOne</cardinality>
		<target>licitacao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
    </association>

</map>
