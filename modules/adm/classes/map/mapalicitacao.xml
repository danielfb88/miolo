<map>
    <moduleName>adm</moduleName>
    <className>mapalicitacao</className>
    <tableName>ad_mapalicitacao</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idLicitacao</attributeName>
        <columnName>idlicitacao</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>item</attributeName>
        <columnName>item</columnName>
        <key>primary</key>
    </attribute>
     <attribute>
        <attributeName>idFornecedor</attributeName>
        <columnName>idfornecedor</columnName>
		<key>primary</key>
    </attribute>

    <attribute>
        <attributeName>preco</attributeName>
        <columnName>preco</columnName>
    </attribute>

    <attribute>
        <attributeName>vencedor</attributeName>
        <columnName>vencedor</columnName>
    </attribute>
    
    <attribute>
        <attributeName>quantidade</attributeName>
        <columnName>quantidade</columnName>
    </attribute>
    
    <attribute>
        <attributeName>marcaModelo</attributeName>
        <columnName>marcamodelo</columnName>
    </attribute>
    
    <attribute>
        <attributeName>importado</attributeName>
        <columnName>importado</columnName>
    </attribute>

    <attribute>
        <attributeName>status</attributeName>
        <columnName>status</columnName>
    </attribute>

    <attribute>
        <attributeName>fornecedor</attributeName>
    </attribute>    
    <attribute>
        <attributeName>licitacao</attributeName>
    </attribute>    
    <attribute>
        <attributeName>itemlicitacao</attributeName>
    </attribute>    

    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>fornecedor</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>fornecedor</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idFornecedor</fromAttribute>
			<toAttribute>idFornecedor</toAttribute>
		</entry>
   </association>
    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>licitacao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>licitacao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
    </association>
    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>itemlicitacao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>itemlicitacao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
		<entry>
			<fromAttribute>item</fromAttribute>
			<toAttribute>item</toAttribute>
		</entry>
    </association>
</map>
