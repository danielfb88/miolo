<map>
    <moduleName>adm</moduleName>
    <className>suplementacaoitem</className>
    <tableName>ad_suplementacaoitem</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idSuplementacaoItem</attributeName>
        <columnName>idsuplementacaoitem</columnName>
		<idgenerator>seq_ad_suplementacaoitem</idgenerator>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idLicitacao</attributeName>
        <columnName>idlicitacao</columnName>
    </attribute>
    <attribute>
        <attributeName>item</attributeName>
        <columnName>item</columnName>
    </attribute>
    <attribute>
        <attributeName>data</attributeName>
        <columnName>data</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>quantidadeSuplementar</attributeName>
        <columnName>quantidadesuplementar</columnName>
    </attribute>
    <attribute>
        <attributeName>valorUnitSuplementar</attributeName>
        <columnName>valorunitsuplementar</columnName>
    </attribute>
    <attribute>
        <attributeName>idSuplementacaoItemRef</attributeName>
        <columnName>idsuplementacaoitemref</columnName>
    </attribute>
    <attribute>
        <attributeName>idRequisicao</attributeName>
        <columnName>idrequisicao</columnName>
    </attribute>

    <attribute>
        <attributeName>requisicao</attributeName>
    </attribute>
    <attribute>
        <attributeName>licitacao</attributeName>
    </attribute>

    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>requisicao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>requisicao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idRequisicao</fromAttribute>
			<toAttribute>idRequisicao</toAttribute>
		</entry>
    </association>
    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>licitacao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>licitacao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLicitacao</fromAttribute>
			<toAttribute>idLicitacao</toAttribute>
		</entry>
    </association>
</map>
