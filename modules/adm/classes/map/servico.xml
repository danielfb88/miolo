<map>
	<moduleName>adm</moduleName>
	<className>servico</className>
	<tableName>ad_servico</tableName>
	<databaseName>sigaept</databaseName>
	<attribute>
		<attributeName>idServico</attributeName>
		<columnName>idservico</columnName>
		<idgenerator>seq_ad_servico</idgenerator>
    	<key>primary</key>
	</attribute>
	<attribute>
		<attributeName>descricao</attributeName>
		<columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>idelemento</attributeName>
		<columnName>idelemento</columnName>
	</attribute>
	<attribute>
		<attributeName>idsubelemento</attributeName>
		<columnName>idsubelemento</columnName>
	</attribute>
	<attribute>
		<attributeName>codServico</attributeName>
		<columnName>codservico</columnName>
	</attribute>
	<attribute>
		<attributeName>ativo</attributeName>
		<columnName>ativo</columnName>
	</attribute>
    
	<attribute>
		<attributeName>subelemento</attributeName>
	</attribute>
	<attribute>
		<attributeName>elemento</attributeName>
	</attribute>

	<association>
      <toClassModule>adm</toClassModule>
      <toClassName>elemento</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>elemento</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idelemento</fromAttribute>
	     <toAttribute>idElemento</toAttribute>
      </entry>
   </association>     

    <association>
      <toClassModule>adm</toClassModule>
      <toClassName>subelemento</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>subelemento</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idelemento</fromAttribute>
	     <toAttribute>idElemento</toAttribute>
      </entry>
	  <entry>
	     <fromAttribute>idsubelemento</fromAttribute>
	     <toAttribute>idSubElemento</toAttribute>
      </entry>
   </association>

</map>
