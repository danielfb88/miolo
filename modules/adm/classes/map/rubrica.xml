<map>
    <moduleName>adm</moduleName>
    <className>rubrica</className>
    <tableName>ad_rubrica</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idRubrica</attributeName>
        <columnName>idrubrica</columnName>
		<key>primary</key>
    </attribute>

    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
    </attribute>

    <attribute>
        <attributeName>rubricasDoOrcamento</attributeName>
    </attribute>
    <attribute>
        <attributeName>tipoReqs</attributeName>
    </attribute>
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>orcamentointerno</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>rubricasDoOrcamento</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idRubrica</fromAttribute>
			<toAttribute>idRubrica</toAttribute>
		</entry>
   </association>
   <!-- By Jeferson Barros -->
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>tiporeq</toClassName>
		<associativeClassModule>adm</associativeClassModule>
		<associativeClassName>reqrubrica</associativeClassName>
		<cardinality>manyToMany</cardinality>
		<target>tipoReqs</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<direction>
			<fromAttribute>rubricas</fromAttribute>
			<toAttribute>tipoReqs</toAttribute>
		</direction>
   </association>
</map>

