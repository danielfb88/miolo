<map>
    <moduleName>adm</moduleName>
    <className>tiporeq</className>
    <tableName>ad_tiporeq</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idTipoReq</attributeName>
        <columnName>idtiporeq</columnName>
		<idgenerator>seq_ad_tiporeq</idgenerator>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>

    <attribute>
        <attributeName>uos</attributeName>
    </attribute>
    
    <attribute>
        <attributeName>rubricas</attributeName>
    </attribute>

    <attribute>
        <attributeName>requisicao</attributeName>
    </attribute>

   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>uo</toClassName>
		<associativeClassModule>adm</associativeClassModule>
		<associativeClassName>executa</associativeClassName>
		<cardinality>manyToMany</cardinality>
		<target>uos</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<direction>
			<fromAttribute>tipoReqs</fromAttribute>
			<toAttribute>uos</toAttribute>
		</direction>
   </association>
   
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>requisicao</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>requisicao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idTipoReq</fromAttribute>
			<toAttribute>idTipoReq</toAttribute>
		</entry>
    </association>
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>rubrica</toClassName>
		<associativeClassModule>adm</associativeClassModule>
		<associativeClassName>reqrubrica</associativeClassName>
		<cardinality>manyToMany</cardinality>
		<target>rubricas</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<direction>
			<fromAttribute>tipoReqs</fromAttribute>
			<toAttribute>rubricas</toAttribute>
		</direction>
   </association>
</map>
