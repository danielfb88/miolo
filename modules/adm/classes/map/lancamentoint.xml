<map>
    <moduleName>adm</moduleName>
    <className>lancamentoint</className>
    <tableName>ad_lancamentoint</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idLancamentoInt</attributeName>
        <columnName>idlancamentoint</columnName>
		<key>primary</key>
		<idgenerator>seq_ad_lancamentoint</idgenerator>
    </attribute>
    <attribute>
        <attributeName>tipoLancamento</attributeName>
        <columnName>tipolancamento</columnName>
    </attribute>
    <attribute>
        <attributeName>data</attributeName>
        <columnName>data</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>valor</attributeName>
        <columnName>valor</columnName>
    </attribute>
    <attribute>
        <attributeName>idUoRequisitante</attributeName>
        <columnName>iduoreq</columnName>
    </attribute>
    <attribute>
        <attributeName>idUoExecutante</attributeName>
        <columnName>iduoexec</columnName>
    </attribute>
    <attribute>
        <attributeName>idRubrica</attributeName>
        <columnName>idrubrica</columnName>
    </attribute>
    <attribute>
        <attributeName>idRequisicao</attributeName>
        <columnName>idrequisicao</columnName>
    </attribute>
    <attribute>
        <attributeName>obs</attributeName>
        <columnName>obs</columnName>
    </attribute>
    <attribute>
        <attributeName>ano</attributeName>
        <columnName>ano</columnName>
    </attribute>
    <attribute>
        <attributeName>idUsuario</attributeName>
        <columnName>idusuario</columnName>
    </attribute>

    <attribute>
        <attributeName>idLicitacao</attributeName>
        <columnName>idlicitacao</columnName>
    </attribute>

    <attribute>
        <attributeName>item</attributeName>
        <columnName>item</columnName>
    </attribute>

    <attribute>
        <attributeName>requisicao</attributeName>
    </attribute>
    <attribute>
        <attributeName>usuario</attributeName>
    </attribute>

   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>requisicao</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>requisicao</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idRequisicao</fromAttribute>
			<toAttribute>idRequisicao</toAttribute>
		</entry>
   </association>
   <association>
		<toClassModule>common</toClassModule>
		<toClassName>usuario</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>usuario</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idUsuario</fromAttribute>
			<toAttribute>idUsuario</toAttribute>
		</entry>
   </association>

</map>
