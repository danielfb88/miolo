<map>
    <moduleName>adm</moduleName>
    <className>requisita</className>
    <tableName>ad_requisita</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idTipoReq</attributeName>
        <columnName>idtiporeq</columnName>
        <key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idUo</attributeName>
        <columnName>iduo</columnName>
        <key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idRubrica</attributeName>
        <columnName>idrubrica</columnName>
    </attribute>
    <attribute>
        <attributeName>ano</attributeName>
        <columnName>ano</columnName>
        <key>primary</key>
    </attribute>
    <attribute>
        <attributeName>ativo</attributeName>
        <columnName>ativo</columnName>
    </attribute>

    <attribute>
        <attributeName>tipoReq</attributeName>
    </attribute>
    
    <attribute>
        <attributeName>uo</attributeName>
    </attribute>
  
    <attribute>
        <attributeName>rubrica</attributeName>
    </attribute>

   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>uo</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>uo</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<entry>
			<fromAttribute>idUo</fromAttribute>
			<toAttribute>idUo</toAttribute>
		</entry>
   </association>
   
   <association>
		<toClassModule>adm</toClassModule>
		<toClassName>tiporeq</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>tipoReq</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<entry>
			<fromAttribute>idTipoReq</fromAttribute>
			<toAttribute>idTipoReq</toAttribute>
		</entry>
   </association>
   
    <association>
		<toClassModule>adm</toClassModule>
		<toClassName>rubrica</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>rubrica</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<entry>
			<fromAttribute>idRubrica</fromAttribute>
			<toAttribute>idRubrica</toAttribute>
		</entry>
   </association>  
</map>
