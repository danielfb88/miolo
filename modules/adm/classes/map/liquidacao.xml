<map>
    <moduleName>adm</moduleName>
    <className>liquidacao</className>
    <tableName>ad_liquidacao</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idLiquidacao</attributeName>
        <columnName>idliquidacao</columnName>
		<idgenerator>seq_ad_liquidacao</idgenerator>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>dataLiquidacao</attributeName>
        <columnName>dataliquidacao</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>obs</attributeName>
        <columnName>obs</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
	<attribute>
        <attributeName>valor</attributeName>
        <columnName>valor</columnName>
    </attribute>
	<attribute>
        <attributeName>pago</attributeName>
        <columnName>pago</columnName>
    </attribute>
	<attribute>
        <attributeName>valorIRRF</attributeName>
        <columnName>valorirrf</columnName>
    </attribute>
	<attribute>
        <attributeName>valorINSS</attributeName>
        <columnName>valorinss</columnName>
    </attribute>
	<attribute>
        <attributeName>valorISS</attributeName>
        <columnName>valoriss</columnName>
    </attribute>
	<attribute>
        <attributeName>notaFiscal</attributeName>
    </attribute>
	<attribute>
        <attributeName>ordemBancaria</attributeName>
    </attribute>

	<association>
		<toClassModule>adm</toClassModule>
		<toClassName>notafiscal</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>notaFiscal</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLiquidacao</fromAttribute>
			<toAttribute>idLiquidacao</toAttribute>
		</entry>
    </association>
	<association>
		<toClassModule>adm</toClassModule>
		<toClassName>ordembancaria</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>ordemBancaria</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idLiquidacao</fromAttribute>
			<toAttribute>idLiquidacao</toAttribute>
		</entry>
    </association>
</map>
