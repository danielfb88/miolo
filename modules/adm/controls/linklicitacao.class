<?
global $MIOLO;
$MIOLO->Uses('controls/linkobject.class','adm');

class LinkLicitacao extends LinkObject
{
    public function __construct($name='', $uo)
    {
		global $MIOLO;
        $MIOLO->UsesBusiness('adm','licitacao');
        parent::__construct($name,$uo,'nothing.jpg');
    }

    public function setObjectByValue($value)
    {
        $this->object = $this->manager->GetBusiness('adm','licitacao');
        $this->object->GetById((int)$value);
    }

    public function setAction()
    {
        $this->hyperlink->SetAction('adm','main:licitacoes',$this->object->idLicitacao);
        $this->imagelink->SetAction('adm','main:licitacoes',$this->object->idLicitacao);
    }

    public function setText()
    {
        $this->hyperlink->SetText($this->object->numero);
    }

    public function setNew($aux = 0)
    {
        // UO do HU
        $uoHU = 325;

		//var_dump($aux);
        if ($aux == "LIVRO") //Licitação de livro
        {
            $this->isNewLink = TRUE;
            $this->hyperlink->SetAction('adm','main:licitacoes:new','',array('item'=>'LIVRO'));
            $this->hyperlink->SetText('Abrir Nova Licitação');
            $this->imagelink->SetAction('adm','main:licitacao:new','',array('item'=>'LIVRO'));
        }
        if ($aux == "PERMANENTE") //Material Permanente 
        {
            $this->isNewLink = TRUE;
            $this->hyperlink->SetAction('adm','main:licitacoes:new','',array('item'=>'PERMANENTE'));
            $this->hyperlink->SetText('Abrir Nova Licitação');
            $this->imagelink->SetAction('adm','main:licitacao:new','',array('item'=>'PERMANENTE'));
        }
        else if ($aux == "SERVICO") //Licitação de serviço
        {
            $this->isNewLink = TRUE;
            $this->hyperlink->SetAction('adm','main:licitacoes:new','',array('item'=>'SERVICO'));
            $this->hyperlink->SetText('Abrir Nova Licitação');
            $this->imagelink->SetAction('adm','main:licitacao:new','',array('item'=>'SERVICO'));
        }
        else if ($aux == $uoHU) //Licitação de compra do HU
        {
            $this->isNewLink = TRUE;
            $this->hyperlink->SetAction('adm','main:licitacoes:new','',array('item'=>$uoHU));
            $this->hyperlink->SetText('Abrir Nova Licitação do HU');
            $this->imagelink->SetAction('adm','main:licitacao:new','',array('item'=>$uoHU));
        }
        else if ($aux == 1) //Licitacao de compra de outras UOs
        {
            $this->isNewLink = TRUE;
            $this->hyperlink->SetAction('adm','main:licitacoes:new');
            $this->hyperlink->SetText('Abrir Nova Licitação');
            $this->imagelink->SetAction('adm','main:licitacao:new');
        }       
    }
}
?>
