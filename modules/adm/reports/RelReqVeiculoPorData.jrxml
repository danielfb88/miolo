	<queryString><![CDATA[SELECT	v.placa,
	modelo,
	datahorasaidaorig as saida,
	prevdatahoraretorno as retorno

FROM	ad_veiculo		v,
	ad_requisicao		r,
	ad_itemreqveiculo	i

WHERE	r.tiporequisicao = '4'
AND	r.idrequisicao = i.idrequisicao
AND	i.placa = v.placa
AND	r.status = '7'
AND	datahorasaidaorig >= TO_DATE($P{pDataIni},'DD-MM-YYYY')

ORDER BY modelo]]></queryString>

