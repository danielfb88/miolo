	<queryString><![CDATA[SELECT
	il.item as Numero, 
	m.descricao as Descricao,  
	ml.quantidade as Quantidade,  
	m.unidade as Unidade,  
	ml.preco as ValorUnit,  
	i.nome as Empresa,
	m.idelemento||m.idsubelemento as Rubrica,
	ir.idrequisicao as Requisicao,
	ml.quantidade * ml.preco  as total
FROM  
	ad_licitacao l,
	ad_itemlicitacao il,  
	ad_itemreq ir,  
	ad_material m,  
	ad_mapalicitacao ml,  
	ad_fornecedor f,  
	cm_instituicao i
WHERE 
	l.numero = $P{licitacao} AND
	il.idlicitacao = l.idlicitacao AND
	il.idlicitacao = ir.idlicitacao AND
	il.item = ir.item AND
	m.idmaterial = ir.idmaterial AND
	ml.item = il.item AND
	ml.idlicitacao = il.idlicitacao AND
	ml.vencedor = 1 AND
	ml.idfornecedor = f.idfornecedor AND
	f.idinstituicao = i.idinstituicao;]]></queryString>

