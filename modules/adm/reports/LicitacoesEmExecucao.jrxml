	<queryString><![CDATA[SELECT
     sum (i.quantpedida * i.precoestimado)
FROM
     ad_material m,
     ad_itemreq i,
     ad_licitacao l
WHERE
     i.idlicitacao = l.idlicitacao
 AND m.idmaterial = i.idmaterial
 AND m.idelemento = '339030'
 AND (l.status = '1'
  OR l.status = '2'
  OR l.status = '3')
 AND l.idtipolicitacao = '1']]></queryString>

