	<queryString><![CDATA[SELECT  m.item,
	m.preco,
	m.quantidade,
	m.marcamodelo,
	(m.preco * m.quantidade) as subtotal,
	i.complemento,
	serv.codservico

FROM	ad_mapalicitacao m,
	ad_itemreqservext i,
	ad_servico serv

