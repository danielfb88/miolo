	<queryString><![CDATA[SELECT 
 coalesce(sum(i.quantpedida*i.precoestimado),0) as sum
FROM
 ad_itemreq i,
 ad_licitacao l,
 ad_material m
WHERE
 I.IDLICITACAO = L.IDLICITACAO AND
 M.IDMATERIAL = I.IDMATERIAL AND
 M.IDELEMENTO = '449052' AND
 (L.STATUS = '1' OR L.STATUS = '2' OR L.STATUS = '3') AND
 L.IDTIPOLICITACAO = '1']]></queryString>

