class frmLicitacaoReqJustlivro extends MForm
{
    protected $licitacao;
    protected $mapa;

	function __construct($licitacao)
	{

        $this->licitacao = $licitacao;
		parent::__construct('Imprimir Relatório de Requisições e Justificativas - Livro');
        $this->mapa = $this->manager->getBusiness('adm','mapalicitacao');
		$this->eventHandler();
	}
	function createFields()
	{
		$this->addButton(new MButton('btnReportReqJust','Exibir Relatório de Requisição e Justificativa - Livro','pdf'));
	}

	function btnReportReqJust_click()
	{
		global $MIOLO;
		$report = new MJasperReport('sigaept');
		$parameters['idlicitacao']   = $this->licitacao->idLicitacao;
		$parameters['SUBREPORT_DIR'] = $MIOLO->GetConf('home.modules');
		$parameters['pURL']          = $MIOLO->GetConf('home.url');
		$parameters['int_pIdUsuario']    = $MIOLO->login->idkey;
		$report->Execute('adm', 'RelJustificativaLivro', $parameters);
	}
}
