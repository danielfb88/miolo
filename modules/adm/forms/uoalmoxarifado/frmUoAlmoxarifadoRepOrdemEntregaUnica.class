class frmUoAlmoxarifadoRepOrdemEntregaUnica extends Form
{
	function __construct()
	{
		global $MIOLO, $module, $self;
		$this->Form('Gerar uma única Ordem de Entrega');
		$this->EventHandler();
	}

	function CreateFields()
	{
		global $MIOLO,$module,$self, $action, $auth;

		$fields = array(
			new TextField('numOrdem','','Numero',10,''),
		);
		$this->SetFields($fields);

		$buttons = array(
			new FormButton('btnPost', 'Gerar Relatório', 'PDF'),
		);
		$this->SetButtons($buttons);
	}

	function btnPost_click()
	{
		global $MIOLO, $module;
		$ui = $MIOLO->GetUI();
		$report = $ui->GetReport($module,'repOrdemEntregaUnica');
		$report->Generate();
	}
}
