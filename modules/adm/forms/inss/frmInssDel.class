class frmInssDel extends MForm
{
	protected $objinss;
	function __construct($objinss)
	{
	$this->objinss = $objinss;
	parent::__construct('Excluir alíquota do INSS');
	$this->EventHandler();
	}
	function CreateFields(){
	global $MIOLO;
	$action_sim = $MIOLO->GetActionURL('adm','main:inss',$this->objinss->idInss, array('form'=>'excluir','conf'=>'sim'));
	$action_nao = $MIOLO->GetActionURL('adm','main:inss',$this->objinss->idInss);
	$conf = $this->GetFormValue('conf');
	if ($conf == 'sim')
	{
	try
	{
		$this->objinss->Delete();
		$MIOLO->Information('Alíquota do INSS Excluída com sucesso',$MIOLO->GetActionURL('adm','main:inss:find'));
	}
	catch (Exception $e)
	{
	$MIOLO->Error($e->GetMessage(),$MIOLO->GetActionURL('adm','main:inss',$this->objinss->idInss));
	}
	}
	else
	{
	$MIOLO->Question("Tem certeza que deseja excluir a alíquota do INSS [{$this->objinss->idInss}]?",$action_sim,$action_nao);
	}
}
}
?>
