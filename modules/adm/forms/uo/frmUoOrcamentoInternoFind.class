$MIOLO->Uses('controls/linkorcamentointerno.class','adm');
/**
 * Formulário básico para seleção de uos
 * 
 */
class frmUoOrcamentoInternoFind extends Form
{

    protected $objUo;

    function __construct($objUo)
    {
        $this->objUo = $objUo;
        $this->Form('Pesquisar Orçamentos Internos');
        $this->EventHandler();
    }

    function CreateFields()
    {
	$opcoesAno = array(
		"2014"=>"2014",
		"2013"=>"2013",
		"2012"=>"2012",
		"2011"=>"2011",
		"201o"=>"2010",
		"2009"=>"2009"
	);
        
        $grid = $this->manager->GetUI()->GetGrid('adm','gridOrcamentoInterno',$this->objUo);
        $this->AddField($grid);
        $this->defaultButton = false;
		
    }
 }
