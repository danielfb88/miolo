	<parameter name="IDGENERO" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	u.descricao as unidade,
	count(distinct(o.idobra)) as qtd_obra,
	count(e.idexemplar) as qtd_exemplares

FROM
	bt_unidade u JOIN
	bt_exemplar e ON (u.idunidade = e.idunidade) JOIN
	bt_obra o ON (e.idobra = o.idobra)

WHERE o.idgenero = $P{IDGENERO}

GROUP BY u.descricao]]>
	</queryString>
	<field name="unidade" class="java.lang.String"/>
	<field name="qtd_obra" class="java.lang.Long"/>
	<field name="qtd_exemplares" class="java.lang.Long"/>
