	<parameter name="DATAINICIO" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="DATAFIM" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ORDERBY" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	o.numerodaobra,
	o.titulo,
	p.nome,
	i.instituicao,
	l.data

FROM
	bt_logoperacao l JOIN
	bt_obra o ON (cast(l.identificador as integer) = o.idobra) LEFT OUTER JOIN
	cm_usuario u ON (l.idusuario = u.idusuario) JOIN
	cm_pessoa p ON (u.idpessoa = p.idpessoa) JOIN
	cm_setor s ON (u.idsetor = s.idsetor) JOIN
	cm_instituicao i ON (s.idinstituicao = i.idinstituicao)
WHERE
	l.idoperacao = 14 AND
	l.data >= $P{DATAINICIO} AND l.data <= $P{DATAFIM}
GROUP BY
	o.numerodaobra, o.titulo, p.nome, l.data, i.instituicao
ORDER BY
	$P!{ORDERBY};]]>
	</queryString>
