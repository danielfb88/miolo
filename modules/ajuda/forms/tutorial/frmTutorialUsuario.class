<?
class frmTutorialUsuario extends MForm
{

    function frmTutorialUsuario()
    {
        global $MIOLO;
    
	parent::__construct('Tutoriais do Usuário');
        $this->SetClose($MIOLO->GetActionURL('ajuda','main'));
        $this->EventHandler();
    }

    function CreateFields()
    {  
       	global $MIOLO;

	$urlProt = $MIOLO->getActionUrl('ajuda','html:files:TutorialProtocolo.pdf');
        $imgProt = $MIOLO->getActionUrl('ajuda','html:images:protocolo.png');

	$urlDitex = $MIOLO->getActionUrl('ajuda','html:files:TutorialDITEX.pdf');
        $imgDitex = $MIOLO->getActionUrl('ajuda','html:images:ditex.png');

	$urlVeic = $MIOLO->getActionUrl('ajuda','html:files:ManualRequisicaoVeiculo.pdf');
        $imgVeic = $MIOLO->getActionUrl('ajuda','html:images:reqveiculo.png');

	/*$urlAlmox = $MIOLO->getActionUrl('ajuda','html:files:Almoxarifado_Manual_do_usuario.pdf');
        $imgAlmox = $MIOLO->getActionUrl('ajuda','html:images:protocolo.png');*/

        $tut1 = new MImageLinkLabel('protocolo', 'Protocolo', $urlProt, $imgProt, NULL );
	$tut2 = new MImageLinkLabel('ditex', 'DITEX',$urlDitex ,$imgDitex, NULL );
	$tut3 = new MImageLinkLabel('veiculo', 'Req. Veículo',$urlVeic ,$imgVeic, NULL );
	//$tut4 = new MImageLinkLabel('almoxarifado', 'Almoxarifado',$urlAlmox ,$imgAlmox, NULL );
	
	$groupTutorial = new MBaseGroup('groupTutorial', 'Arquivos',array($tut1, $tut2, $tut3),'horizontal','css');
        
	$fields = array(
		$groupTutorial,
		new MSeparator(),
		);
        $this->SetFields($fields);
        $this->defaultButton = false;
   }	
}
?>
