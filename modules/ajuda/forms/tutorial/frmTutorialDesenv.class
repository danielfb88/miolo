<?
class frmTutorialDesenv extends MForm
{

    function frmTutorialDesenv()
    {
        global $MIOLO;
    
	parent::__construct('Tutoriais de Desenvolvimento');
        $this->SetClose($MIOLO->GetActionURL('ajuda','main'));
        $this->EventHandler();
    }

    function CreateFields()
    {  
	global $MIOLO;

	$urlReqVeic = $MIOLO->getActionUrl('ajuda','html:files:ManualRequisicaoVeiculo-ParteTecnica.pdf');
        $imgReqVeic = $MIOLO->getActionUrl('ajuda','html:images:reqveiculo.png');

	$urlRel = $MIOLO->getActionUrl('ajuda','html:files:TutorialEZPDF.pdf');
        $imgRel = $MIOLO->getActionUrl('ajuda','html:images:pdf.png');

        $tut1 = new MImageLinkLabel('requisicao', 'Req. Veículo', $urlReqVeic, $imgReqVeic, NULL );
	$tut2 = new MImageLinkLabel('relatorio', 'Gerar Relatório EZPDF',$urlRel ,$imgRel, NULL );
	
	$groupTutorial = new MBaseGroup('groupTutorial', 'Arquivos',array($tut1, $tut2),'horizontal','css');
        
	$fields = array(
		$groupTutorial,
		new MSeparator(),
		);
        $this->SetFields($fields);
        $this->defaultButton = false;

    }
}
?>
