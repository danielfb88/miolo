<?php
class frmGeradorTabelasFind extends MForm
{
	function __construct()
	{
		parent::__construct('Pesquisar Tabelas Cadastradas para Gerador de Relatório');
		$this->SetClose($this->manager->GetActionURL('common','main:mad'));
		$this->EventHandler();
	}
	function CreateFields()
	{
		$ui = $this->manager->GetUI();
		$grid = $ui->GetGrid('common','gridGeradorTabela');
		$grid->SetTitle(NULL);
		$lnkNew = new MLink('lnkNew','&nbsp;','','Inserir Tabela');
		$lnkNew->SetAction('common','main:mad:geradortabelas:new');
		$findButton = new MButtonFind('');
		$findButton->SetName('btnFind');
		$fields = array(
                    array(
                        new MTextField('nome','','Tabela',30,'Nome físico'),
                        $findButton,
                        $lnkNew
                    ),
                $grid
                );
		$this->SetFields($fields);
		$this->defaultButton = false;
	}
}
?>
