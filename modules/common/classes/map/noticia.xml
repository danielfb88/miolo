<map>
	<moduleName>common</moduleName>
	<className>noticia</className>
	<tableName>cm_noticia</tableName>
	<databaseName>common</databaseName>
	<attribute>
		<attributeName>idNoticia</attributeName>
		<columnName>idnoticia</columnName>
		<key>primary</key>
        <idgenerator>seq_cm_noticia</idgenerator>
	</attribute>
	<attribute>
		<attributeName>noticia</attributeName>
		<columnName>noticia</columnName>
	</attribute>
	<attribute>
		<attributeName>idSistema</attributeName>
		<columnName>idsistema</columnName>
	</attribute>
	<attribute>
		<attributeName>dataNoticia</attributeName>
		<columnName>dtnoticia</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>common</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>sistema</attributeName>
	</attribute>

   <association>
		<toClassModule>common</toClassModule>
		<toClassName>sistema</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>sistema</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idSistema</fromAttribute>
			<toAttribute>idSistema</toAttribute>
		</entry>
   </association>
</map>
