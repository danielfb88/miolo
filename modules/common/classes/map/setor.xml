      <associativeClassName>organograma</associativeClassName>
      <cardinality>manyToMany</cardinality>
      <target>organogramas</target>
          <retrieveAutomatic>false</retrieveAutomatic>
          <saveAutomatic>false</saveAutomatic>
          <deleteAutomatic>true</deleteAutomatic>
          <direction>
             <fromAttribute>setores</fromAttribute>
             <toAttribute>organogramas</toAttribute>
      </direction>
   </association>

