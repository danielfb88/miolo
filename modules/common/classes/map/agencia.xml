<map>
	<moduleName>common</moduleName>
	<className>agencia</className>
	<tableName>cm_agencia</tableName>
	<databaseName>common</databaseName>
	<attribute>
		<attributeName>idAgencia</attributeName>
		<columnName>idagenc</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
		<key>primary</key>
	</attribute>
	<attribute>
		<attributeName>idBanco</attributeName>
		<columnName>idbanco</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
		<key>primary</key>
	</attribute>
	<attribute>
		<attributeName>nome</attributeName>
		<columnName>nome</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>banco</attributeName>
	</attribute>

    <association>
		<toClassModule>common</toClassModule>
		<toClassName>banco</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>banco</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<inverse>true</inverse>
		<entry>
			<fromAttribute>idBanco</fromAttribute>
			<toAttribute>idBanco</toAttribute>
		</entry>
    </association>


</map>