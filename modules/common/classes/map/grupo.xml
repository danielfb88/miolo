<map>
	<moduleName>common</moduleName>
	<className>grupo</className>
	<tableName>cm_grupoacesso</tableName>
	<databaseName>common</databaseName>
	<attribute>
		<attributeName>idGrupo</attributeName>
		<columnName>idgrupo</columnName>
		<key>primary</key>
        <idgenerator>seq_cm_grupoacesso</idgenerator>
	</attribute>
	<attribute>
		<attributeName>grupo</attributeName>
		<columnName>grupo</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>usuarios</attributeName>
	</attribute>
	<attribute>
		<attributeName>acesso</attributeName>
	</attribute>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>usuario</toClassName>
      <associativeClassModule>common</associativeClassModule>
      <associativeClassName>grupousuario</associativeClassName>
      <cardinality>manyToMany</cardinality>
      <target>usuarios</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <direction>
	     <fromAttribute>grupos</fromAttribute>
	     <toAttribute>usuarios</toAttribute>
      </direction>
   </association>

   <association>
		<toClassModule>common</toClassModule>
		<toClassName>acesso</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>acesso</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idGrupo</fromAttribute>
			<toAttribute>idGrupo</toAttribute>
		</entry>
   </association>

</map>
