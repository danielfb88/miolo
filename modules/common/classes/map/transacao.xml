<map>
	<moduleName>common</moduleName>
	<className>transacao</className>
	<tableName>cm_transacao</tableName>
	<databaseName>common</databaseName>
	<attribute>
		<attributeName>idTransacao</attributeName>
		<columnName>idtrans</columnName>
		<key>primary</key>
        <idgenerator>seq_cm_transacao</idgenerator>
	</attribute>
	<attribute>
		<attributeName>transacao</attributeName>
		<columnName>transacao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>idSistema</attributeName>
		<columnName>idsistema</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
		<attributeName>acesso</attributeName>
	</attribute>
	<attribute>
		<attributeName>sistema</attributeName>
	</attribute>

   <association>
		<toClassModule>common</toClassModule>
		<toClassName>acesso</toClassName>
		<cardinality>oneToMany</cardinality>
		<target>acesso</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idTransacao</fromAttribute>
			<toAttribute>idTransacao</toAttribute>
		</entry>
   </association>

   <association>
		<toClassModule>common</toClassModule>
		<toClassName>sistema</toClassName>
		<cardinality>oneToOne</cardinality>
		<target>sistema</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<saveAutomatic>false</saveAutomatic>
		<entry>
			<fromAttribute>idSistema</fromAttribute>
			<toAttribute>idSistema</toAttribute>
		</entry>
   </association>
</map>
