<map>
    <moduleName>common</moduleName>
    <className>grupousuario</className>
    <tableName>cm_grpusuario</tableName>
    <databaseName>common</databaseName>
    <attribute>
        <attributeName>idUsuario</attributeName>
        <columnName>idusuario</columnName>
   	</attribute>
    <attribute>
        <attributeName>idGrupo</attributeName>
        <columnName>idgrupo</columnName>
   	</attribute>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>usuario</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>usuarios</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>true</saveAutomatic>
	  <entry>
	     <fromAttribute>idUsuario</fromAttribute>
	     <toAttribute>idUsuario</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>grupo</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>grupos</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>true</saveAutomatic>
	  <entry>
	     <fromAttribute>idGrupo</fromAttribute>
	     <toAttribute>idGrupo</toAttribute>
      </entry>
   </association>

</map>
