<map>
   <moduleName>common</moduleName>
   <className>acesso</className>
   <tableName>cm_acesso</tableName>
   <databaseName>common</databaseName>
   <attribute>
      <attributeName>idTransacao</attributeName>
	  <columnName>idtrans</columnName>
      <key>primary</key>
   </attribute>
   <attribute>
      <attributeName>idGrupo</attributeName>
	  <columnName>idgrupo</columnName>
      <key>primary</key>
   </attribute>
   <attribute>
      <attributeName>direito</attributeName>
	  <columnName>direito</columnName>
   </attribute>
   <attribute>
      <attributeName>transacao</attributeName>
   </attribute>
   <attribute>
      <attributeName>grupo</attributeName>
   </attribute>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>transacao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>transacao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idTransacao</fromAttribute>
	     <toAttribute>idTransacao</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>grupo</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>grupo</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idGrupo</fromAttribute>
	     <toAttribute>idGrupo</toAttribute>
      </entry>
   </association>
</map>

