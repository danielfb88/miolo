<map>
    <moduleName>common</moduleName>
    <className>organograma</className>
    <tableName>cm_organograma</tableName>
    <databaseName>common</databaseName>
    <attribute>
        <attributeName>idTipoOrganograma</attributeName>
        <columnName>idtipoorganograma</columnName>
    </attribute>
    <attribute>
        <attributeName>idSetor</attributeName>
        <columnName>idsetor</columnName>
    </attribute>
    <attribute>
        <attributeName>idSetorPai</attributeName>
        <columnName>idsetorpai</columnName>
    </attribute>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>setor</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>setores</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idSetor</fromAttribute>
	     <toAttribute>idSetor</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>tipoorganograma</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>organogramas</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idTipoOrganograma</fromAttribute>
	     <toAttribute>idTipoOrganograma</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>setor</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>setorPai</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <deleteAutomatic>false</deleteAutomatic>
	  <entry>
	     <fromAttribute>idSetorPai</fromAttribute>
	     <toAttribute>idSetor</toAttribute>
      </entry>
   </association>
</map>