/**
 * Formulário de exclusão tipoLicAfast
 */
class frmTipoLicAfastDel extends Form
{
	/**
	 * Objeto tipoLicAfast
	 * @access protected
	 * @var BusinessRhTipoLicAfast
	 */
	protected $objTipoLicAfast;

    function __construct($objTipoLicAfast)
    {
		$this->objTipoLicAfast = $objTipoLicAfast;
        parent::__construct('Excluir Tipo LicAfast');
        $this->EventHandler();
    }

    function CreateFields()
    {
        global $MIOLO;

		$action_sim = $MIOLO->GetActionURL('rh','main:tipolicafast',$this->objTipoLicAfast->idTipoLicAfast,array('form'=>'excluir','conf'=>'sim'));
		$action_nao = $MIOLO->GetActionURL('rh','main:tipolicafast',$this->objTipoLicAfast->idTipoLicAfast);

		$conf = $this->GetFormValue('conf');

		if ($conf == 'sim')
		{
			$this->objTipoLicAfast->Delete();
			if (!$this->objTipoLicAfast->CheckError())
			{
				$MIOLO->Information('Tipo LicAfast excluída com sucesso',$MIOLO->GetActionURL('rh','main:tipolicafast:find'));
			}
			else
			{
				foreach( $this->objTipoLicAfast->GetErrors() as $e )
				{
					$err .= " $e ";
				}
				$MIOLO->Error("Informe o seguinte erro à equipe do CGCO: ".$e,$MIOLO->GetActionURL('rh','main:tipolicafast',$this->objTipoLicAfast->idTipoLicAfast));
			}
					
	   }
	   else
	   {
		   $MIOLO->Question("Tem certeza que deseja excluir esse tipo de LicAfast?",$action_sim,$action_nao);
		}
	   
    }


 }
