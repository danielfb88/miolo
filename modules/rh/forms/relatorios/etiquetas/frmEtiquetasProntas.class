class frmEtiquetasProntas extends MForm
{


	function __construct()
    {
		parent::__construct('Etiquetas prontas');
		$this->SetClose($this->manager->GetActionURL('rh','main:etiquetas'));
		$this->EventHandler();
    }


    function CreateFields()
    {
		$fields = array(
			new MText('l1','As etiquetas abaixo estão em formato TXT, use as instruções da tela anterir para imprimi-las.','black',false),
			new MText('l1','','black',false),	
			new FormButton('btnServidoresAtivos','Servidores Ativos (TAs) - Todos aprox. 1.105','PDF'),
			new MText('l2','','black',false),
			new FormButton('btnServidoresInativos1e2Graus','Servidores Inativos 1º e 2º Graus - Aprox. 105','PDF'),
			new MText('l3','','black',false),
			new FormButton('btnPensionistas1e2Graus','Pensionistas 1º e 2º Graus - Aprox. 14','PDF'),
		);
        $this->SetFields($fields);

		$buttons = Array
		(
			new FormButton('btnBack','Voltar'),
		);
		$this->SetButtons($buttons);
    } 


    function btnBack_click()
    {
		global $page;
		$go = $this->manager->GetActionURL('rh','main:etiquetas');
		$page->Redirect($go);
	}


    function btnServidoresInativos1e2Graus_click()
    {
        	$report = new MJasperReport('ufjf');
			$report->Execute('rh','etiquetasprontas/RH_EtiquetaTXT35_Servidores_Inativos_1e2Graus',$parameters,null,'TXT');
	}
    
	
	function btnServidoresAtivos_click()
    {
        	$report = new MJasperReport('ufjf');
			$report->Execute('rh','etiquetasprontas/RH_EtiquetaTXT35_Servidores_Ativos',$parameters,null,'TXT');
	}

	
	function btnPensionistas1e2Graus_click()
    {
        	$report = new MJasperReport('ufjf');
			$report->Execute('rh','etiquetasprontas/RH_EtiquetaTXT35_Pensionistas_1e2Graus',$parameters,null,'TXT');
	}
}
?>
