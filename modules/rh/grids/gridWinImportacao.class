/**
 * Grid WinImportacao
 * Grid para listagem de arquivos de processamento
 * @package br.org.miolo.ufjf.rh.grids
 */

class gridWinImportacao extends MDataGrid2
{
    protected $objArquivo;


    function __construct()
    {
        global $MIOLO, $self;
        $perms = $MIOLO->getPerms();

        $columns = array(
            new MDataGridColumn('nome'             , 'Arquivo'    , 'left'  , true, '40%', true),
            new MDataGridColumn('tamanho'          , 'Tamanho'    , 'center', true, '10%', true),
            new MDataGridColumn('mesReferencia'    , 'Mês'        , 'center', true,  '5%', true),
            new MDataGridColumn('anoReferencia'    , 'Ano'        , 'center', true,  '5%', true),
            new MDataGridColumn('dataGravacao'     , 'Enviado em' , 'center', true, '20%', true),
            new MDataGridColumn('Processado'       , 'Processado' , 'center', true, '10%', false),
            new MDataGridColumn('dataProcessamento', 'Dt Process.', 'center', true, '20%', true)
        );
        $this->objArquivo = $MIOLO->GetBusiness('common','arquivo');
        $href_grid = $MIOLO->GetActionURL('rh',$self);
        $query = $this->objArquivo->ListAll();
        $query->AddFilter('nome','like','f-esp-pensao%');
        $query->AddFilter('nome','like','siapefitaesp%','OR');
        $query->ApplyFilter(); // SAMIRA 
        parent::__construct($query, $columns, $href_grid,0);
        $this->SetTitle('Arquivos de processamento em lote.');
        $this->SetWidth('100%');
        $this->SetFilter(false);
        $this->SetIndex(0);
    } // fim do construtor


    function GenerateHeader()
    {
        return NULL;
    }


    function GenerateFooter()
    {
        if (!$this->data)
             $footer[] = $this->GenerateEmptyMsg();
           // $footer[] = new Separator();
           // $footer[] = $this->GenerateControls();
        return $footer;
    }


}
?>
