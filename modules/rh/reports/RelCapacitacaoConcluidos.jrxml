	<queryString><![CDATA[Select 
	s.idsetor,
	s.nomesetor,
	i.idvinculo,
	p.nome,
	c.nome as curso,
	m.nome as modulo 
	
FROM 
	cm_setor s,
	rh_capacitacaoinsc i,
	cm_pessoa p, 
	rh_capacitacaocurso c, 
	rh_capacitacaomodulo m, 
	rh_provimento pr,
	rh_funcionario f, 
	rh_vinculo v 
	
WHERE 
	i.idvinculo = pr.idvinculo 
	and 
	pr.idsetor = s.idsetor 
	and 
	i.idvinculo = v.idvinculo 
	and 
	v.idfuncionario = f.idfuncionario 
	and 
	f.idpessoa = p.idpessoa 
	and 
	i.idmodulo = m.idmodulo 
	and 
	m.idcurso=c.idcurso 
	and 
	TO_CHAR(TO_DATE(ts,'DD/MM/YYYY'),'YYYY') = $P{ano}
