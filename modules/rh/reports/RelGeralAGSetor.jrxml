	<queryString><![CDATA[SELECT 
	rh_funcionario.idfuncionario,
	rh_vinculo.idvinculo,
	cm_pessoa.nome,
	rh_provimento.regimejuridico,
	rh_situacaofuncional.descricao as situacao,
	rh_cargo.descricao as cargo,
	rh_jornada.descricao as jornada,
	cm_setor.nomesetor as setor 

FROM 
	(((((((rh_funcionario INNER JOIN rh_vinculo ON 
		((rh_funcionario.idfuncionario=rh_vinculo.idfuncionario))) 
		INNER JOIN rh_provimento ON 
			((rh_vinculo.idvinculo=rh_provimento.idvinculo))) 
		INNER JOIN cm_pessoa ON 
			((rh_funcionario.idpessoa=cm_pessoa.idpessoa))) 
		INNER JOIN rh_situacaofuncional ON 
			((rh_provimento.idsituacao=rh_situacaofuncional.idsituacao))) 
		INNER JOIN rh_cargo ON 
			((rh_provimento.idgrupocargo=rh_cargo.idgrupocargo) 
			AND (rh_provimento.idcargo=rh_cargo.idcargo))) 
		INNER JOIN rh_jornada ON 
			((rh_provimento.idjornada=rh_jornada.idjornada))) 
		INNER JOIN cm_setor ON 
			((rh_provimento.idsetor=cm_setor.idsetor))) 
WHERE 
	(TO_CHAR(RH_PROVIMENTO.DATAFIM,'YYYY/MM/DD') is NULL) 
	ORDER BY cm_setor.nomesetor ASC,cm_pessoa.nome ASC]]></queryString>

