	<queryString><![CDATA[SELECT 
	TO_CHAR(RH_PROVIMENTO.DATAINICIO,'DD/MM/YYYY') as dataInicio,
	cm_setor.siglasetor || '  ' || cm_setor.nomesetor as sigla,
	rh_cargo.descricao as cargo,
	rh_ambiente.descricao as ambiente
	
FROM 
	(((rh_provimento RIGHT JOIN rh_ambiente ON ((rh_provimento.idambiente=rh_ambiente.idambiente))) 
	INNER JOIN cm_setor ON ((rh_provimento.idsetor=cm_setor.idsetor))) 
	INNER JOIN rh_cargo ON ((rh_provimento.idgrupocargo=rh_cargo.idgrupocargo) AND (rh_provimento.idcargo=rh_cargo.idcargo))) 

WHERE (rh_provimento.idvinculo = $P{matricula}) ORDER BY rh_provimento.datainicio ASC]]></queryString>

