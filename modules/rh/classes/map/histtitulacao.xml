<?xml version="1.0" standalone="yes"?>
<map>
    <moduleName>rh</moduleName>
    <className>histtitulacao</className>
    <tableName>rh_histtitulacao</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idHistTitulacao</attributeName>
        <columnName>idhisttitulacao</columnName>
		<key>primary</key>
		<idgenerator>seq_rh_histtitulacao</idgenerator>
    </attribute>
    <attribute>
        <attributeName>idFuncionario</attributeName>
        <columnName>idfuncionario</columnName>
    </attribute>
    <attribute>
        <attributeName>idTitulacao</attributeName>
        <columnName>idtitulacao</columnName>
    </attribute>
    <attribute>
        <attributeName>dataFim</attributeName>
        <columnName>datafim</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>entidade</attributeName>
        <columnName>entidade</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>tituloTrabalhoFinal</attributeName>
        <columnName>titulotrabalhofinal</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>tipoTrabalhoFinal</attributeName>
        <columnName>tipotrabalhofinal</columnName>
    </attribute>
    <attribute>
        <attributeName>areaConcentracao</attributeName>
        <columnName>areaconcentracao</columnName>
    </attribute>
    <attribute>
        <attributeName>dataincentivo</attributeName>
        <columnName>dataincentivo</columnName>
    </attribute>    
    <attribute>
        <attributeName>nomecurso</attributeName>
        <columnName>nomecurso</columnName>
    </attribute>
    <attribute>
        <attributeName>idEscolaridadePCCTAE</attributeName>
        <columnName>idescolaridadepcctae</columnName>
    </attribute>
    <attribute>
        <attributeName>escolaridadePCCTAE</attributeName>
    </attribute>
    <attribute>
        <attributeName>funcionario</attributeName>
    </attribute>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>funcionario</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>funcionario</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>false</inverse>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>

    <association>
      <toClassModule>rh</toClassModule>
      <toClassName>escolaridadepcctae</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>escolaridadePCCTAE</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <entry>
	     <fromAttribute>idEscolaridadePCCTAE</fromAttribute>
	     <toAttribute>idEscolaridadePCCTAE</toAttribute>
      </entry>
   </association>

</map>
