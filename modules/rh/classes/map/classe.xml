<?xml version="1.0" standalone="yes"?> 
<map>
    <moduleName>rh</moduleName>
    <className>classe</className>
    <tableName>rh_classe</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idClasse</attributeName>
        <columnName>idclasse</columnName>
	<idgenerator>seq_rh_classe</idgenerator>
        <key>primary</key>
    </attribute>
    <attribute>
        <attributeName>classe</attributeName>
        <columnName>classe</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>nome</attributeName>
        <columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>categoria</attributeName>
        <columnName>categoria</columnName>
    </attribute>
    <attribute>
        <attributeName>ordemprogressao</attributeName>
        <columnName>ordemprogressao</columnName>
    </attribute>
    
    <attribute>
        <attributeName>referencias</attributeName>
    </attribute>
    

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>referencia</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>referencias</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <deleteAutomatic>true</deleteAutomatic>
	  <saveAutomatic>true</saveAutomatic>
	  <orderAttribute>
	      <orderAttributeName>nivelPadrao</orderAttributeName>
		  <orderAttributeDirection>ascend</orderAttributeDirection>
      </orderAttribute>
	  <indexAttribute>
	      <indexAttributeName>idReferencia</indexAttributeName>
      </indexAttribute>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idClasse</fromAttribute>
	     <toAttribute>idClasse</toAttribute>
      </entry>
   </association>

</map>
