<?xml version="1.0" standalone="yes"?> 
<map>
    <moduleName>rh</moduleName>
    <className>grupocargo</className>
    <tableName>rh_grupocargo</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idGrupoCargo</attributeName>
        <columnName>idgrupocargo</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
        <attributeName>cargos</attributeName>
    </attribute>
	<attribute>
        <attributeName>vagas</attributeName>
    </attribute>
	
	<association>
      <toClassModule>rh</toClassModule>
      <toClassName>cargo</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>cargos</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <deleteAutomatic>false</deleteAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	    <inverse>true</inverse>
      <orderAttribute>
	      <orderAttributeName>idCargo</orderAttributeName>
		  <orderAttributeDirection>ascend</orderAttributeDirection>
      </orderAttribute>
	  <indexAttribute>
	      <indexAttributeName>idCargo</indexAttributeName>
      </indexAttribute>	
	  <entry>
	     <fromAttribute>idGrupoCargo</fromAttribute>
	     <toAttribute>idGrupoCargo</toAttribute>
     </entry>
   </association>

	<association>
      <toClassModule>rh</toClassModule>
      <toClassName>vaga</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>vagas</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <deleteAutomatic>false</deleteAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	    <inverse>true</inverse>
	  <indexAttribute>
	      <indexAttributeName>numeroVaga</indexAttributeName>
      </indexAttribute>	
	  <entry>
	     <fromAttribute>idGrupoCargo</fromAttribute>
	     <toAttribute>idGrupoCargo</toAttribute>
     </entry>
   </association>
</map>
