<?xml version="1.0" standalone="yes"?>
<map>
    <moduleName>rh</moduleName>
    <className>situacaofuncional</className>
    <tableName>rh_situacaofuncional</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idSituacao</attributeName>
        <columnName>idsituacao</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>regimeJuridico</attributeName>
        <columnName>regimejuridico</columnName>
    </attribute>
    <attribute>
        <attributeName>ocorrencias</attributeName>
    </attribute>
    
   <association>
		<toClassModule>rh</toClassModule>
		<toClassName>ocorrencia</toClassName>
		<associativeClassModule>rh</associativeClassModule>
		<associativeClassName>situacaoocorrencia</associativeClassName>
		<cardinality>manyToMany</cardinality>
		<target>ocorrencias</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<direction>
			<fromAttribute>situacaoFuncional</fromAttribute>
			<toAttribute>ocorrencias</toAttribute>
		</direction>
   </association>
</map>
