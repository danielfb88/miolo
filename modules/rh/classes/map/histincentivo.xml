<?xml version="1.0" standalone="yes"?>
<map>
    <moduleName>rh</moduleName>
    <className>histincentivo</className>
    <tableName>rh_histincentivo</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idHistIncentivo</attributeName>
        <columnName>idhistincentivo</columnName>
		<key>primary</key>
		<idgenerator>seq_rh_histincentivo</idgenerator>
    </attribute>
    <attribute>
        <attributeName>idHistFormacao</attributeName>
        <columnName>idhistformacao</columnName>
    </attribute>
    <attribute>
        <attributeName>idHistTitulacao</attributeName>
        <columnName>idhisttitulacao</columnName>
    </attribute>
    <attribute>
        <attributeName>idPercQualificacao</attributeName>
        <columnName>idpercqualificacao</columnName>
    </attribute>
    <attribute>
        <attributeName>idVinculo</attributeName>
        <columnName>idvinculo</columnName>
    </attribute>
    <attribute>
        <attributeName>relacao</attributeName>
        <columnName>relacao</columnName>
    </attribute>
    <attribute>
        <attributeName>percentualAplicado</attributeName>
        <columnName>percentualaplicado</columnName>
    </attribute>
    <attribute>
