        <columnName>dataincentivo</columnName>
        <converter>
            <converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
	</attribute>
	<attribute>
        <attributeName>histFormacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>histTitulacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>percQualificacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>vinculo</attributeName>
    </attribute>
    <association>
      <toClassModule>rh</toClassModule>
      <toClassName>histformacao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>histFormacao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
      <entry>
	     <fromAttribute>idHistFormacao</fromAttribute>
	     <toAttribute>idHistFormacao</toAttribute>
      </entry>
   </association>
    <association>
      <toClassModule>rh</toClassModule>
      <toClassName>histtitulacao</toClassName>
