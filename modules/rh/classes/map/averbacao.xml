<?xml version="1.0" standalone="yes"?> 
<map>
    <moduleName>rh</moduleName>
    <className>averbacao</className>
    <tableName>rh_averbacao</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idAverbacao</attributeName>
        <columnName>idaverbacao</columnName>
        <idgenerator>seq_rh_averbacao</idgenerator>
        <key>primary</key>
    </attribute>
    <attribute>
        <attributeName>processo</attributeName>
        <columnName>processo</columnName>
    </attribute>
    <attribute>
        <attributeName>dataInicio</attributeName>
        <columnName>datainicio</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>dataFim</attributeName>
        <columnName>datafim</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>sigaept</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>totalDias</attributeName>
        <columnName>totaldias</columnName>
    </attribute>
    <attribute>
        <attributeName>tipoTempo</attributeName>
        <columnName>tipotempo</columnName>
    </attribute>
    <attribute>
        <attributeName>instituicao</attributeName>
        <columnName>instituicao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>idVinculo</attributeName>
        <columnName>idvinculo</columnName>
    </attribute>

    <attribute>
        <attributeName>finalidades</attributeName>
    </attribute>
    
    <attribute>
        <attributeName>desaverbacoes</attributeName>
    </attribute>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>averbfinalidade</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>finalidades</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <deleteAutomatic>false</deleteAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <indexAttribute>
	      <indexAttributeName>idAverbFinalidade</indexAttributeName>
      </indexAttribute>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idAverbacao</fromAttribute>
	     <toAttribute>idAverbacao</toAttribute>
      </entry>
   </association>
   
   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>desaverbacao</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>desaverbacoes</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <deleteAutomatic>false</deleteAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <indexAttribute>
	      <indexAttributeName>idDesaverbacao</indexAttributeName>
      </indexAttribute>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idAverbacao</fromAttribute>
	     <toAttribute>idAverbacao</toAttribute>
      </entry>
   </association>

</map>
