    <retrieveAutomatic>false</retrieveAutomatic>
    <saveAutomatic>false</saveAutomatic>
    <entry>
      <fromAttribute>idcurso</fromAttribute>
        <toAttribute>curso</toAttribute>
    </entry>
  </association>
-->  
  <association>
    <toClassModule>rh</toClassModule>
    <toClassName>cargoconfianca</toClassName>
    <cardinality>oneToOne</cardinality>
    <target>cargoconfianca</target>
    <retrieveAutomatic>false</retrieveAutomatic>
    <saveAutomatic>false</saveAutomatic>
    <entry>
      <fromAttribute>idcargoconfianca</fromAttribute>
        <toAttribute>idCargoConfianca</toAttribute>
    </entry>
  </association>
  
</map>
