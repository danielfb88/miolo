<?xml version="1.0" standalone="yes"?>
<map>
    <moduleName>rh</moduleName>
    <className>funcao</className>
    <tableName>rh_funcao</tableName>
    <databaseName>sigaept</databaseName>

    <attribute>
        <attributeName>idfuncao</attributeName>
        <columnName>idfuncao</columnName>
        <key>primary</key>
        <idgenerator>seq_rh_funcao</idgenerator>
    </attribute>

    <attribute>
        <attributeName>idnivel</attributeName>
        <columnName>idnivel</columnName>
    </attribute>
    <attribute>
        <attributeName>idsetor</attributeName>
        <columnName>idsetor</columnName>
    </attribute>
    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>mandato</attributeName>
        <columnName>mandato</columnName>
    </attribute>
    <attribute>
        <attributeName>datainicio</attributeName>
        <columnName>datainicio</columnName>
        <converter>
            <converterName>DateConverter</converterName>
            <parameter>
                <parameterName>database</parameterName>
                <parameterValue>sigaept</parameterValue>
            </parameter>
        </converter>
    </attribute>
    <attribute>
        <attributeName>datafim</attributeName>
        <columnName>datafim</columnName>
        <converter>
            <converterName>DateConverter</converterName>
            <parameter>
                <parameterName>database</parameterName>
                <parameterValue>sigaept</parameterValue>
            </parameter>
        </converter>
    </attribute>
    <attribute>
        <attributeName>idpublicacaoinicio</attributeName>
        <columnName>idpublicacaoinicio</columnName>
    </attribute>
    <attribute>
        <attributeName>idpublicacaofim</attributeName>
        <columnName>idpublicacaofim</columnName>
    </attribute>
    <attribute>
        <attributeName>idcurso</attributeName>
        <columnName>idcurso</columnName>
    </attribute>
    <attribute>
        <attributeName>idcargoconfianca</attributeName>
        <columnName>idcargoconfianca</columnName>
    </attribute>

    <attribute>
        <attributeName>nivelfuncao</attributeName>
    </attribute>
    <attribute>
        <attributeName>setor</attributeName>
    </attribute>
    <attribute>
        <attributeName>publicacaoinicio</attributeName>
    </attribute>
    <attribute>
        <attributeName>publicacaofim</attributeName>
    </attribute>
    <attribute>
        <attributeName>ocupacaofuncao</attributeName>
    </attribute>
    <attribute>
        <attributeName>curso</attributeName>
    </attribute>
    <attribute>
        <attributeName>cargoconfianca</attributeName>
    </attribute>

    <association>
      <toClassModule>rh</toClassModule>
      <toClassName>nivelfuncao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>nivelfuncao</target>
      <retrieveAutomatic>false</retrieveAutomatic>
      <saveAutomatic>false</saveAutomatic>
      <entry>
         <fromAttribute>idnivel</fromAttribute>
         <toAttribute>idnivel</toAttribute>
      </entry>
    </association>
   <association>
      <toClassModule>common</toClassModule>
      <toClassName>setor</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>setor</target>
      <retrieveAutomatic>false</retrieveAutomatic>
      <deleteAutomatic>false</deleteAutomatic>
      <saveAutomatic>false</saveAutomatic>
      <entry>
         <fromAttribute>idsetor</fromAttribute>
         <toAttribute>idSetor</toAttribute>
      </entry>
   </association>
   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>publicacao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>publicacaoinicio</target>
      <retrieveAutomatic>false</retrieveAutomatic>
      <saveAutomatic>false</saveAutomatic>
      <entry>
         <fromAttribute>idpublicacaoinicio</fromAttribute>
         <toAttribute>idPublicacao</toAttribute>
      </entry>
   </association>
   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>publicacao</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>publicacaofim</target>
      <retrieveAutomatic>false</retrieveAutomatic>
      <saveAutomatic>false</saveAutomatic>
      <entry>
         <fromAttribute>idpublicacaofim</fromAttribute>
         <toAttribute>idPublicacao</toAttribute>
      </entry>
   </association>

  <association>
    <toClassModule>rh</toClassModule>
    <toClassName>ocupacaofuncao</toClassName>
    <cardinality>oneToMany</cardinality>
    <target>ocupacaofuncao</target>
    <retrieveAutomatic>false</retrieveAutomatic>
    <saveAutomatic>false</saveAutomatic>
    <entry>
      <fromAttribute>idfuncao</fromAttribute>
        <toAttribute>idfuncao</toAttribute>
    </entry>
  </association>
  
