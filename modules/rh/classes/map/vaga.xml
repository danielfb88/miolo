<?xml version="1.0" standalone="yes"?> 
<map>
    <moduleName>rh</moduleName>
    <className>vaga</className>
    <tableName>rh_vaga</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>numeroVaga</attributeName>
        <columnName>numerovaga</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idGrupoCargo</attributeName>
        <columnName>idgrupocargo</columnName>
    </attribute>
    <attribute>
        <attributeName>idCargo</attributeName>
        <columnName>idcargo</columnName>
    </attribute>
    <attribute>
        <attributeName>dataInicio</attributeName>
        <columnName>datainicio</columnName>
    </attribute>
    <attribute>
        <attributeName>dataFim</attributeName>
        <columnName>datafim</columnName>
    </attribute>
    <attribute>
        <attributeName>origem</attributeName>
        <columnName>origem</columnName>
    </attribute>
	<attribute>
        <attributeName>idSetor</attributeName>
        <columnName>idsetor</columnName>
    </attribute>
    <attribute>
        <attributeName>cargo</attributeName>
    </attribute>
	<attribute>
		<attributeName>ocupacaoVagas</attributeName>
	</attribute>
	<attribute>
		<attributeName>setor</attributeName>
	</attribute>
	

	<association>
      <toClassModule>rh</toClassModule>
      <toClassName>cargo</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>cargo</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idCargo</fromAttribute>
	     <toAttribute>idCargo</toAttribute>
      </entry>
	  <entry>
	     <fromAttribute>idGrupoCargo</fromAttribute>
	     <toAttribute>idGrupoCargo</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>ocupacaovaga</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>ocupacaoVagas</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>numeroVaga</fromAttribute>
	     <toAttribute>numeroVaga</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>setor</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>setor</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idSetor</fromAttribute>
	     <toAttribute>idSetor</toAttribute>
      </entry>
   </association>

</map>
