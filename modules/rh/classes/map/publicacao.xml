	<attribute>
        <attributeName>diplomaLegal</attributeName>
    </attribute>
	
	<association>
	<toClassModule>rh</toClassModule>
	<toClassName>diplomalegal</toClassName>
	<cardinality>oneToOne</cardinality>
	<target>diplomaLegal</target>
		<retrieveAutomatic>true</retrieveAutomatic>
		<entry>
			<fromAttribute>idDiplomaLegal</fromAttribute>
			<toAttribute>idDiplomaLegal</toAttribute>
		</entry>
	</association>
	
