<?xml version="1.0" standalone="yes"?>
<map>
    <moduleName>rh</moduleName>
    <className>funcionario</className>
    <tableName>rh_funcionario </tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idFuncionario</attributeName>
        <columnName>idfuncionario</columnName>
		<key>primary</key>
		<converter>
			<converterName>TrimCaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>escolaridade</attributeName>
        <columnName>escolaridade</columnName>
		<converter>
			<converterName>CaseConverter</converterName>
			<parameter>
				<parameterName>case</parameterName>
				<parameterValue>upper</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>ufEmpregoAnterior</attributeName>
        <columnName>ufempregoanterior</columnName>
    </attribute>
    <attribute>
        <attributeName>dataPrimeiroEmprego</attributeName>
        <columnName>dataprimeiroemprego</columnName>
		<converter>
			<converterName>DateConverter</converterName>
			<parameter>
				<parameterName>database</parameterName>
				<parameterValue>common</parameterValue>
			</parameter>
		</converter>
    </attribute>
    <attribute>
        <attributeName>idPessoa</attributeName>
        <columnName>idpessoa</columnName>
    </attribute>
    <attribute>
        <attributeName>pessoa</attributeName>
    </attribute>
    <attribute>
        <attributeName>dependentes</attributeName>
    </attribute>
    <attribute>
        <attributeName>titulacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>formacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>capacitacao</attributeName>
    </attribute>
    <attribute>
        <attributeName>vinculo</attributeName>
    </attribute>

    <attribute>
        <attributeName>escolaridadeTabelaGeral</attributeName>
    </attribute>

    <association>
                <toClassModule>common</toClassModule>
                <toClassName>tabelageral</toClassName>
                <cardinality>oneToOne</cardinality>
                <target>escolaridadeTabelaGeral</target>
                <retrieveAutomatic>true</retrieveAutomatic>
                <saveAutomatic>false</saveAutomatic>
                <entry>
                        <fromAttribute>escolaridade</fromAttribute>
                        <toAttribute>item1</toAttribute>
                </entry>
   </association>

   <association>
      <toClassModule>common</toClassModule>
      <toClassName>pessoa</toClassName>
      <cardinality>oneToOne</cardinality>
      <target>pessoa</target>
	  <retrieveAutomatic>true</retrieveAutomatic>
	  <saveAutomatic>true</saveAutomatic>
	  <entry>
	     <fromAttribute>idPessoa</fromAttribute>
	     <toAttribute>idpessoa</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>dependente</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>dependentes</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <orderAttribute>
	      <orderAttributeName>parentesco</orderAttributeName>
		  <orderAttributeDirection>ascend</orderAttributeDirection>
      </orderAttribute>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>histtitulacao</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>titulacao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>
   
   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>histcapacitacao</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>capacitacao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>histformacao</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>formacao</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
      <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>

   <association>
      <toClassModule>rh</toClassModule>
      <toClassName>vinculo</toClassName>
      <cardinality>oneToMany</cardinality>
      <target>vinculo</target>
	  <retrieveAutomatic>false</retrieveAutomatic>
	  <saveAutomatic>false</saveAutomatic>
	  <inverse>true</inverse>
	  <entry>
	     <fromAttribute>idFuncionario</fromAttribute>
	     <toAttribute>idFuncionario</toAttribute>
      </entry>
   </association>
</map>
