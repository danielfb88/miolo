    </attribute>
    <attribute>
        <attributeName>situacaoFuncional</attributeName>
    </attribute>
    <attribute>
        <attributeName>tipoLicAfasts</attributeName>
    </attribute>
	
	<association>
    	<toClassModule>rh</toClassModule>
    	<toClassName>situacaofuncional</toClassName>
    	<associativeClassModule>rh</associativeClassModule>
    	<associativeClassName>situacaoocorrencia</associativeClassName>
