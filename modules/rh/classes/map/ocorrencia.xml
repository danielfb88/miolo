<?xml version="1.0" standalone="yes"?> 
<map>
    <moduleName>rh</moduleName>
    <className>ocorrencia</className>
    <tableName>rh_ocorrencia</tableName>
    <databaseName>sigaept</databaseName>
    <attribute>
        <attributeName>idGrupoOcorrencia</attributeName>
        <columnName>idgrupoocorrencia</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>idOcorrencia</attributeName>
        <columnName>idocorrencia</columnName>
		<key>primary</key>
    </attribute>
    <attribute>
        <attributeName>descricao</attributeName>
        <columnName>descricao</columnName>
	<converter>
		<converterName>CaseConverter</converterName>
		<parameter>
			<parameterName>case</parameterName>
			<parameterValue>upper</parameterValue>
		</parameter>
	</converter>
    </attribute>
    <attribute>
        <attributeName>mnemonico</attributeName>
