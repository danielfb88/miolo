		<target>tipoLicAfasts</target>
		<retrieveAutomatic>false</retrieveAutomatic>
		<saveAutomatic>true</saveAutomatic>
		<direction>
			<fromAttribute>ocorrencias</fromAttribute>
			<toAttribute>tipoLicAfasts</toAttribute>
		</direction>
   </association>

	
</map>
